{
  "version": 2,
  "builds": [
    {
      "src": "api/comments.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/comments.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
